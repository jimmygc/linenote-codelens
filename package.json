{
	"name": "linenote-codelens",
	"repository": "https://github.com/jimmygc/linenote-codelens.git",
	"displayName": "Note Line in Codelens",
	"description": "note line in codelens",
	"version": "0.0.2",
	"publisher": "jimmygc",
	"engines": {
		"vscode": "^1.26.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "linenotecodelens.openNote",
				"title": "LineNote: Open note"
			},
			{
				"command": "linenotecodelens.removeNote",
				"title": "LineNote: Delete note"
			},
			{
				"command": "linenotecodelens.moveNote",
				"title": "LineNote: Move note to"
			},
			{
				"command": "linenotecodelens.moveDownNote",
				"title": "LineNote: Move note down"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "linenotecodelens.openNote",
					"group": "linenote@1"
				},
				{
					"command": "linenotecodelens.removeNote",
					"group": "linenote@2"
				},
				{
					"command": "linenotecodelens.moveNote",
					"group": "linenote@4"
				},
				{
					"command": "linenotecodelens.moveDownNote",
					"group": "linenote@3"
				}
			]
		},
		"configuration": {
			"properties": {}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.26.0",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"eslint": "^7.21.0",
		"tslint": "^5.8.0",
		"typescript": "^4.2.2",
		"vscode": "^0.9.9"
	},
	"dependencies": {
		"sqlite": "^4.0.23",
		"sqlite3": "^5.0.2"
	}
}
